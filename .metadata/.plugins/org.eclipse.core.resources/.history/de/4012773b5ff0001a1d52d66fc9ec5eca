package cap01;

import java.util.Scanner;

public class HolaMundoNombre {
	public static void main(String[] args) {
		Scanner dataForConsole = new Scanner(System.in);
		String newData = dataForConsole.nextLine();
		System.out.println("Este fue el valor ingresado por el usuario");
		System.out.println(newData);
	}
}
